{
	"name": "shadcn-svelte-extras",
	"packageManager": "pnpm@10.4.1",
	"version": "1.23.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"build:registry": "jsrepo build",
		"generate-i18n": "pnpm exec paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"@fontsource-variable/inter": "^5.2.5",
		"@fontsource/geist-mono": "^5.2.5",
		"@lucide/svelte": "^0.486.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@shikijs/langs": "^3.2.1",
		"@shikijs/themes": "^3.2.1",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"autoprefixer": "^10.4.21",
		"bits-ui": "1.3.15",
		"clsx": "^2.1.1",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.4.1",
		"globals": "^16.0.0",
		"isomorphic-dompurify": "^2.22.0",
		"jsrepo": "^1.45.1",
		"mode-watcher": "^0.5.1",
		"package-manager-detector": "^1.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"runed": "^0.25.0",
		"shiki": "^3.2.1",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"svelte-easy-crop": "^4.0.1",
		"svelte-sonner": "^0.3.28",
		"svelte-tel-input": "^3.6.0",
		"svelte-toolbelt": "^0.7.1",
		"sveltekit-superforms": "^2.24.0",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.28.0",
		"valibot": "1.0.0",
		"vaul-svelte": "1.0.0-next.3",
		"vite": "^6.2.4",
		"yeezy-dates": "^1.0.1",
		"@inlang/paraglide-js": "2.0.11",
		"@inlang/cli": "^3.0.0"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
